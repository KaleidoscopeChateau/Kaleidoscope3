          toggles: []
          prompts: []
          singleValues: []
        - name: Welcome
          local: Welcome
          text: 
          texts:
          - name: Instructions
            text: '

              Hello and welcome to Audio Diaries!


              Before
              getting started make sure to check the Setup tab for the initial setup
              instructions.

'
